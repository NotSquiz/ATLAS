# ATLAS Configuration
# Copy this to .env and fill in your values

# Ollama settings (optional - defaults work for local)
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODEL=atlas-local

# Database
ATLAS_DB_PATH=./data/atlas.db

# Claude API (for cloud fallback)
# Note: If using Claude Code CLI with Max subscription, leave this empty
# ANTHROPIC_API_KEY=

# Garmin credentials (Phase 1)
# GARMIN_EMAIL=
# GARMIN_PASSWORD=
